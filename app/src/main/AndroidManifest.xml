<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.travelguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pingyao"
            android:label="@string/pingyao"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Wutai_Mountain"
            android:label="@string/wutai_mountain"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Taiyuan"
            android:label="@string/taiyuan"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Datong"
            android:label="@string/datong"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Hukou"
            android:label="@string/hukou"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>